# Backend API URL
# Use empty string to leverage Vite's proxy in development
# For production, set to your actual backend URL
VITE_BACKEND_URL=
REACT_APP_BACKEND_URL=

# WebSocket URL
# Use relative path to leverage Vite's proxy in development
VITE_WS_URL=/ws
REACT_APP_WS_URL=/ws

# Environment
VITE_ENV=development
REACT_APP_ENV=development

# Feature Flags
REACT_APP_ENABLE_3D=true
REACT_APP_ENABLE_AUDIO=true
VITE_ENABLE_AUDIO=true
REACT_APP_ENABLE_VISUAL_EDITS=true